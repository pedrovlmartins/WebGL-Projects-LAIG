        <MATERIAL id="null" />    
        <TEXTURE id="null" />       			

        <ROTATION axis="y" angle="-6" />
        <TRANSLATION x="0" y="-0.3" z="0" />
		
        <DESCENDANTS>
            <NODEREF id="packStructure" />
            <NODEREF id="cigarettes" />
		</DESCENDANTS>
