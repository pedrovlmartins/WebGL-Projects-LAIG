	<LIGHT id="No Smoking">                                   
        <enable value ="1" />                   	       
        <position x="-4" y="8" z="20" w="1" />       		   
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />      
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />       
    </LIGHT>
</LIGHTS>

<!-- Textures -->
<TEXTURES>
	<TEXTURE id="floorTexture">
        <file path="images/floor.jpg" />               
        <amplif_factor s="6" t="4" />                           
    </TEXTURE>

	<TEXTURE id="cigaretteTexture">
        <file path="images/cigarette.jpg" />   
        <amplif_factor s="1" t="1" />                           
    </TEXTURE>
	
	<TEXTURE id="marlboroFrenteTexture">
        <file path="images/marlboroFrente.jpg" />               
        <amplif_factor s="5" t="4" />                           
    </TEXTURE>
	
	<TEXTURE id="marlboroLadoTexture">
        <file path="images/marlboroLado.jpg" />               
        <amplif_factor s="5" t="4" />                           
    </TEXTURE>
	
	<TEXTURE id="marlboroTopoTexture">
        <file path="images/marlboroTopo.jpg" />               
        <amplif_factor s="1" t="1" />                           
    </TEXTURE>
	
	<TEXTURE id="smokingKillsTexture">
        <file path="images/smokingKills.jpg" />               
        <amplif_factor s="5" t="4" />                           
    </TEXTURE>
	
	<TEXTURE id="noSmokingSignTexture">
        <file path="images/noSmoking.jpg" />               
        <amplif_factor s="1" t="1" />                           
    </TEXTURE>
</TEXTURES>


<MATERIALS>
    <MATERIAL id="cigaretteMaterial">                                  
        <shininess value="1" />
        <specular r="0.7" g="0.7" b="0.7" a="1" />        
        <diffuse r="0.9" g="0.9" b="0.9" a="0.9" />       
        <ambient r="0.3" g="0.3" b="0.3" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />      
    </MATERIAL>

    <MATERIAL id="floorMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />       
        <ambient r="0.647059" g="0.164706" b="0.164706" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>

    <MATERIAL id="adMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0" g="0" b="0" a="1" />       
        <ambient r="1" g="1" b="1" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>	

    <MATERIAL id="noSmokingMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />       
        <ambient r="0.2" g="0" b="0" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>	

    <MATERIAL id="insidePaperMaterial">                                  
        <shininess value="1" />
        <specular r="0.4" g="0.4" b="0.4" a="1" />        
        <diffuse r="0.9" g="0.9" b="0.9" a="0.9" />       
        <ambient r="0" g="0" b="0" a="1" />         
        <emission r="0.2" g="0.2" b="0.2" a="1" />         
    </MATERIAL>
	
   <MATERIAL id="insideTopMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />       
        <ambient r="1" g="0.972549" b="0.972549" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />       
    </MATERIAL>	

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />       
        <ambient r="0.4" g="0.4" b="0.4" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>
</MATERIALS>

<!-- The nodes -->
<NODES>
    <ROOT id="root" />     

    <!-- The root node -->
    <NODE id="root">       
        <MATERIAL id="defaultMaterial" />         
        <TEXTURE id="null" />  

        <DESCENDANTS>
            <NODEREF id="floor" />
			<NODEREF id="lonelyCigarette" />
			<NODEREF id="pack"/>
			<NODEREF id="smokingAd" />
        </DESCENDANTS>
    </NODE>

	<!-- The floor, consisting of a patch with degrees 2/1 -->
	<NODE id="floor">      
        <MATERIAL id="floorMaterial" />    
        <TEXTURE id="floorTexture" />

        <SCALE sx="6" sy="6" sz="7" />
        <TRANSLATION x="3" y="0" z="2.3" />
        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
			<LEAF type="patch" args="50 50">
				<CPLINE>
					<CPOINT xx="-2" yy="-2" zz="-1" ww="1" />
					<CPOINT xx="-10" yy="3" zz="-1" ww="1" />
				</CPLINE>	
				<CPLINE>
					<CPOINT xx="-2" yy="-2" zz="0" ww="1" />
					<CPOINT xx="-10" yy="3" zz="0" ww="1" />
				</CPLINE>
				<CPLINE>
					<CPOINT xx="2" yy="-2" zz="0" ww="1" />
					<CPOINT xx="2" yy="2" zz="0" ww="1" />
				</CPLINE>			
			</LEAF>
		</DESCENDANTS>
    </NODE>

	<!-- The no-smoking advertisements -->
    <NODE id="smokingAd">      
        <MATERIAL id="adMaterial" />    
        <TEXTURE id="smokingKillsTexture" />       			

        <DESCENDANTS>
            <NODEREF id="smokingSign" />
			<NODEREF id="smokingSphere" />
		</DESCENDANTS>
    </NODE>

	<!-- The 'smoking kills' advertisement -->
    <NODE id="smokingSign">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />       			

        <SCALE sx="3" sy="3" sz="1" />		
        <TRANSLATION x="3" y="0" z="0" />

        <DESCENDANTS>
			<LEAF  type="rectangle" args="0 4 5 0" />        
		</DESCENDANTS>
    </NODE>

	<!-- The 'no smoking here' sign sphere -->
    <NODE id="smokingSphere">      
        <MATERIAL id="noSmokingMaterial" />    
        <TEXTURE id="noSmokingSignTexture" />       			

        <ROTATION axis="y" angle="55" />
        <TRANSLATION x="-14" y="10.1" z="8" />

        <DESCENDANTS>
			<LEAF  type="sphere" args="2 50 50" />        
		</DESCENDANTS>
    </NODE>

